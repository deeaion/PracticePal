<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="574.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.exampracticeapp.Controller.ImportDataController">
    <children>
        <Label layoutX="29.0" layoutY="43.0" prefHeight="43.0" prefWidth="181.0" text="IMPORT DATA FROM TXT FILE" />
        <Button fx:id="btn_ChooseFile" layoutX="232.0" layoutY="98.0" mnemonicParsing="false" onAction="#handleUploadingFile" prefHeight="43.0" prefWidth="163.0" text="File Chooser" />
        <Label layoutX="29.0" layoutY="166.0" prefHeight="17.0" prefWidth="103.0" text="Directly insert Text" />
        <TextArea fx:id="txt_ToInsert" layoutX="18.0" layoutY="197.0" prefHeight="229.0" prefWidth="563.0" />
        <Button fx:id="btn_import" layoutX="270.0" layoutY="526.0" mnemonicParsing="false" onAction="#handleImport" text="IMPORT" />
      <HBox layoutX="62.0" layoutY="461.0" prefHeight="33.0" prefWidth="504.0">
         <children>
            <Label prefHeight="33.0" prefWidth="79.0" text="SUBJECT:" />
            <TextField fx:id="txt_subject" />
            <Label prefHeight="33.0" prefWidth="79.0" text="CHAPTER" />
            <TextField fx:id="txt_chapter" />
         </children>
      </HBox>
      <Button fx:id="btn_ShowExample" layoutX="476.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleShowExample" text="SHOW EXAMPLE" />
    </children>
</AnchorPane>
